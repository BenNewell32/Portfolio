    <script class="js">
      function fade(){
        var documentv = $(document),
            fadeElement = $('.fade-scroll');
                document.on('scroll', function (){
          var currentScrollPosition = documentv.scrollTop();
          fadeElement.each(function(){
            var $this = $(this),
                elementOffsetTop = $this.offset().top;
            if (currentScrollPosition > elementOffsetTop){
            $this.css('opacity', 1-(currentScrollPosition-elementOffsetTop)/400)
            }
          })
        })

      }
    </script>

    
